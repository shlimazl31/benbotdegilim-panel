{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/api/*", "/assets/*"],
  "routes": [
    {
      "src": "/privacy",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/terms",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
} 